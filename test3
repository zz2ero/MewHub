local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/bigbootylatinas/mew/main/main"))()
local GUI =
    Mercury:Create {
    Name = "MewHub",
    Size = UDim2.fromOffset(600, 400),
    Theme = Mercury.Themes.Serika,
    Link = "https://mewhub.com"
}

local Tab = GUI:Tab{
	Name = "Pokemon Ultima",
	Icon = "rbxassetid://10516768544"
}

Tab:Button{
	Name = "Load Script",
	Description = nil,
	Callback = function() 
	
	loadstring(game:HttpGet("https://raw.githubusercontent.com/ScripterHaxer/Script/main/Ultima"))()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/ScripterHaxer/Script/main/UltimaGiver"))()

	    end
}





